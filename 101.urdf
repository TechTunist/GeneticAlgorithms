<!-- create a simple single link urdf object -->

<?xml version="1.01"?>
<robot name="my_first">
    <link name="base_link">
        <visual>
            <geometry>
                <cylinder radius="0.6" length="0.25"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.6" length="0.25"/>
            </geometry>
        </collision>
    </link>
</robot>