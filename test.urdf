<?xml version="1.1"?>
<robot name="my_first">
    <link name="base_link">
        <visual>
            <geometry>
                <cylinder radius="0.6" length="0.25"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.6" length="0.25"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.25"/>
            <inertia ixx="0.0003" iyy="0.0003" izz="0.0003" ixy="0" ixz="0" iyz="0"/>
        </inertial>
    </link>
    <link name="sub_link">
        <visual>
            <geometry>
                <cylinder radius="0.6" length="0.25"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="1" length="1.25"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.25"/>
            <inertia ixx="0.0003" iyy="0.0003" izz="0.0003" ixy="0" ixz="0" iyz="0"/>
        </inertial>
    </link>
    <joint name="base_to_sub" type="fixed">
        <parent link="base_link"/>
        <child link="sub_link"/>
        
    </joint>
</robot>