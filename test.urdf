<?xml version="1.1"?>
<robot name="myfirst">
    <link name="base_link">
        <visual>
            <geometry>
                <cylinder length="0.6" radius="0.25"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.6" radius="0.25"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.25"/>
            <inertia ixx="0.0003" iyy="0.0003" izz="0.0003" ixy="0" ixz="0" iyz="0"/>
        </inertial>
    </link>
    
    
    <link name="sub_link">
        <visual>
            <geometry>
                <cylinder length="1" radius="0.1"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <cylinder length="1" radius="1.25"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.25"/>
            <inertia ixx="0.0003" iyy="0.0003" izz="0.0003" ixy="0" ixz="0" iyz="0"/>
        </inertial>
    </link>

    <link name="sub_link2">
        <visual>
            <geometry>
                <cylinder length="5" radius="0.1"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <cylinder length="5" radius="1.25"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.25"/>
            <inertia ixx="0.0003" iyy="0.0003" izz="0.0003" ixy="0" ixz="0" iyz="0"/>
        </inertial>
    </link>

    <joint name="base_to_sub" type="revolute">
        <parent link="base_link"/>
        <child link="sub_link"/>
        <axis xyz="1 0 0"/>
        <limit effort="10" lower="0" upper="10" velocity="1"/>
        <origin rpy="0 0 0" xyz="0.5 0 0"/>
    </joint>

    <joint name="base_to_sub2" type="revolute">
        <parent link="base_link"/>
        <child link="sub_link2"/>
        <axis xyz="1 0 0"/>
        <limit effort="10" lower="0" upper="10" velocity="1"/>
        <origin rpy="0 0 0" xyz="0.5 0 0"/>
    </joint>

</robot>